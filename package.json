{
  "name": "vue3-timepicker",
  "version": "1.0.0-beta.2",
  "description": "A dropdown time picker (hour|minute|second) for Vue 3.x, with flexible time format support",
  "author": "Phoenix Wong <phoenixyfwong@gmail.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rollup -c rollup.config.js"
  },
  "module": "dist/VueTimepikcer.esm.js",
  "main": "dist/VueTimepikcer.cjs.js",
  "unpkg": "dist/VueTimepikcer.global.js",
  "css": "dist/VueTimepicker.css",
  "files": [
    "dist/*.{js,css,map}",
    "src/*",
    "*.json",
    "*.md"
  ],
  "peerDependencies": {
    "vue": "3.3.4"
  },
  "dependencies": {
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.8.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "postcss": "^8.2.4",
    "rollup": "^2.35.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "vue",
    "vue3",
    "time",
    "picker",
    "dropdown",
    "input",
    "UI"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phoenixwong/vue3-timepicker.git"
  }
}
